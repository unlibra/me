---
interface Props {
  class?: string;
}

const { class: className } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 100 100"
  fill="none"
  shape-rendering="auto"
  class={className}
>
  <mask id="viewboxMask"
    ><rect width="100" height="100" rx="0" ry="0" x="0" y="0" fill="#fff"
    ></rect></mask
  >
  <g mask="url(#viewboxMask)">
    <rect fill="#f88c49" width="100" height="100" x="0" y="0"></rect>
    <g transform="translate(-4, 3) rotate(8 50 70)">
      <path
        d="M95 53.33C95 29.4 74.85 10 50 10S5 29.4 5 53.33V140h90V53.33Z"
        fill="#69d2e7"></path>
      <g transform="translate(29 33)">
        <g transform="translate(5, -14) rotate(-16 21 21)">
          <g transform="translate(0 4)">
            <path
              d="M13 8c0 2.2-1.34 4-3 4s-3-1.8-3-4 1.34-4 3-4 3 1.8 3 4ZM35 8c0 2.2-1.34 4-3 4s-3-1.8-3-4 1.34-4 3-4 3 1.8 3 4Z"
              fill="#ffffff"></path>
          </g>
          <g transform="translate(6 24)">
            <path
              d="M15 10C6.79 10 3.02 3.88 4.22 3.12 5.42 2.35 6.1 6.6 15 6.49c8.9-.12 9.58-4.23 10.78-3.37C26.98 3.98 23.21 10 15 10Z"
              fill="#ffffff"></path>
          </g>
        </g>
      </g>
    </g>
  </g>
</svg>
