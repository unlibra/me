# Site configuration for mirror sites
# Copy this file to .env and set your own values

# Site URL (used for OGP and sitemap)
PUBLIC_SITE_URL=https://unlibra.com

# Site title (displayed in browser tab and header)
PUBLIC_SITE_TITLE=unlibra.com

# Profile name (displayed on the site)
PUBLIC_PROFILE_NAME=unlibra

# Profile username (displayed as @username)
PUBLIC_PROFILE_USERNAME=unlibra

# Disable robots crawling (set to "true" to disallow all robots)
# Default: false (robots are allowed)
DISABLE_ROBOTS=false
